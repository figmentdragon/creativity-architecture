{
  "name": "website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "sass --embed-sources --embed-source-map src/style.scss:style.css src/assets:assets src/scss:assets/css",
    "server": "serve --listen 3000",
    "production": "sass -i src/style.scss:style.css -w",
    "server-watch": "concurrently serve --listen 3000 -w sass -i src/style.scss:style.css",
    "watch": "sass src/style.scss:style.css src/assets:assets src/docs:docs -w",
    "interactive": "sass -i -c --updated src/style.scss:style.css",
    "concurrently": "npm-run-all production compile interactive watch server"
  },
  "author": "cjmt",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.45.0",
    "scss": "^0.2.4",
    "serve": "^13.0.2"
  },
  "dependencies": {
    "materialize-css": "^1.0.0",
    "stylesheet-writer": "^1.0.1",
    "the-new-css-reset": "^1.4.2"
  }
}
